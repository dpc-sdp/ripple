{
  "name": "with-tide-api",
  "version": "1.23.1",
  "description": "Example site for showing how to use @dpc-sdp/ripple-tide-api",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"nuxt\"",
    "build": "run-script-os",
    "build:default": "nuxt build --modern=client",
    "build:win32": "nuxt build --modern=client",
    "start": "run-script-os",
    "start:default": "nuxt start --modern=client",
    "start:win32": "nuxt start --modern=client",
    "start:build": "nuxt build --modern=client && nuxt start --modern=client"
  },
  "dependencies": {
    "@dpc-sdp/ripple-nuxt": "1.23.1",
    "@dpc-sdp/ripple-nuxt-ui": "1.23.1",
    "@dpc-sdp/ripple-tide-api": "^1.23.1",
    "@dpc-sdp/ripple-tide-landing-page": "^1.23.1",
    "nuxt": "2.15.2",
    "run-script-os": "^1.1.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  },
  "browserslist": [
    "> 1%",
    "IE 11",
    "last 2 versions",
    "not ie <= 8"
  ],
  "nodemonConfig": {
    "watch": [
      "./tide-api",
      "./../../packages/ripple-tide-landing-page"
    ]
  }
}
