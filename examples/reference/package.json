{
  "name": "reference",
  "version": "1.23.1",
  "description": "Example site for showing how to use @dpc-sdp/ripple-tide-api",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"nuxt\"",
    "dev-debug": "BASIC_AUTH=0 node --inspect node_modules/.bin/nuxt",
    "build": "run-script-os",
    "build:default": "nuxt build --modern=client",
    "build:win32": "nuxt build --modern=client",
    "start": "run-script-os",
    "start:default": "nuxt start --modern=client",
    "start:win32": "nuxt start --modern=client",
    "start:build": "nuxt build --modern=client && nuxt start --modern=client",
    "test:dev": "cross-env SEARCH_API_BASE_URL=http://localhost:3001 API_URL=http://localhost:3001 NODE_ENV=test start-server-and-test dev http://localhost:3000 \"cypress open\""
  },
  "dependencies": {
    "@dpc-sdp/ripple-nuxt": "^1.27.2",
    "@dpc-sdp/ripple-tide-api": "^1.27.2",
    "@dpc-sdp/ripple-tide-landing-page": "^1.27.2",
    "@nuxt/typescript-runtime": "^2.1.0",
    "dotenv": "^5.0.1",
    "nuxt": "2.15.2",
    "run-script-os": "^1.1.3"
  },
  "devDependencies": {
    "@dpc-sdp/ripple-test-tools": "^1.23.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "nodemon": "^2.0.12",
    "start-server-and-test": "^1.14.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "step_definitions": "test/integration/features",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "./../../test-results/cucumber",
      "filePrefix": "reference-"
    }
  },
  "browserslist": [
    "> 1%",
    "IE 11",
    "last 2 versions",
    "not ie <= 8"
  ]
}
